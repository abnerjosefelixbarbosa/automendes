  "components": {
    "schemas": {
      "ExceptionDTO": {
        "type": "object",
        "properties": {
          "localDateTime": {
            "type": "string",
            "format": "date-time"
          },
          "status": {
            "type": "integer",
            "format": "int32"
          },
          "message": {
            "type": "string"
          },
          "path": {
            "type": "string"
          }
        }
      },
      "VehicleRequestDTO": {
        "required": [
          "boxgearType",
          "modelName",
          "price",
          "vehicleType"
        ],
        "type": "object",
        "properties": {
          "plate": {
            "maxLength": 20,
            "minLength": 0,
            "type": "string"
          },
          "modelName": {
            "maxLength": 30,
            "minLength": 0,
            "type": "string"
          },
          "price": {
            "type": "number"
          },
          "boxgearType": {
            "type": "string",
            "enum": [
              "AUTO",
              "MANUAL"
            ]
          },
          "vehicleType": {
            "type": "string",
            "enum": [
              "CAR",
              "MOTOCYCLE"
            ]
          }
        }
      },
      "VehicleResponseDTO": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "plate": {
            "type": "string"
          },
          "price": {
            "type": "number"
          },
          "boxgearType": {
            "type": "string",
            "enum": [
              "AUTO",
              "MANUAL"
            ]
          },
          "vehicleType": {
            "type": "string",
            "enum": [
              "CAR",
              "MOTOCYCLE"
            ]
          },
          "modelName": {
            "type": "string"
          }
        }
      },
      "ModelRequestDTO": {
        "required": [
          "brandName",
          "name"
        ],
        "type": "object",
        "properties": {
          "name": {
            "maxLength": 30,
